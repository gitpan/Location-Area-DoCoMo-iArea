use strict;
use Test::More tests => 3538;
BEGIN { use_ok 'Location::Area::DoCoMo::iArea' }

while (<DATA>) {
  if ($_ =~ /^\d+,([^,]+),([^,]+),(\d{3}),(\d{2}),/)
  {
    my ($region,$pref,$aid,$said) = ($1,$2,$3,$4);
    my $obj = Location::Area::DoCoMo::iArea->create_iarea($aid.$said);
    is $obj->region, $region;
    is substr($obj->prefecture,0,4), substr($pref,0,4);

    my @nexts = $obj->get_nextarea;
    foreach my $next (@nexts)
    {
      my $check = $next->id;
      my @invs = $next->get_nextarea;
      foreach my $inv (@invs)
      {
        $check = $aid.$said if ($inv->id eq $aid.$said);
      }
      is $check, $aid.$said;
    }
  }
}

__END__
